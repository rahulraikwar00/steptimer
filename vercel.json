{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.cjs",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/public/**"] }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.cjs"
    }
  ]
}
